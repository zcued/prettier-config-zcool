{
  "$schema": "http://json.schemastore.org/prettierrc",
  "tabWidth": 2,
  "tabs": false,
  "semi": false,
  "printWidth": 120,
  "singleQuote": true,
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "lf"
}
